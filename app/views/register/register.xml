<Page>
	<StackLayout>
		<TextField id="firstName" text="{{ firstName }}" hint="First name" />
		<TextField id="lastName" text="{{ lastName }}" hint="Last name" />
		<TextField id="email" text="{{ email }}"  hint="Email Address" keyboardType="email" />
		<TextField id="phone" text="{{ phone }}"  hint="Phone number" keyboardType="number" />

		<Button text="Send order" tap="send"/>
	</StackLayout>
</Page>

